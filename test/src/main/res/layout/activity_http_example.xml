<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.droidutils.test.http.HttpExampleActivity">


    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/test_multi_threading"
        android:id="@+id/test_multi_threading_button"
        android:layout_above="@+id/used_cache_button"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/used_cache_button"
        android:id="@+id/used_cache_button"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/used_request_with_limit_button"
        android:id="@+id/used_request_with_limit_button"
        android:layout_below="@+id/used_cache_button"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
