<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.droidutils.sample" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".backstack.BackStackExampleActivity"
            android:label="@string/title_activity_back_stack" >
        </activity>
        <activity
            android:name=".http.HttpExampleActivity"
            android:label="@string/title_activity_http_example" >
        </activity>
        <activity
            android:name=".json.JsonExampleActivity"
            android:label="@string/title_activity_json_example" >
        </activity>
    </application>

</manifest>
